#Defining the quick sort function 'qiksort'

def qiksort(list):
    """ Quicksort a list
    """
    if list == []: 
        return []
    pivot = list[0]
    lesser = qiksort([x for x in list[1:] if x < pivot])
    greater = qiksort([x for x in list[1:] if x >= pivot])
    return lesser + [pivot] + greater
